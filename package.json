{
  "name": "thinkmoreforum-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "build-export": "next build && next export",
    "test": "jest -c jest.config.js --colors --passWithNoTests",
    "test:watch": "jest -c jest.config.js --colors --passWithNoTests --watch",
    "test:coverage": "jest -c jest.config.js --colors --coverage --passWithNoTests",
    "lint:eslint": "eslint --ext .jsx,.js ./pages && eslint --ext .jsx,.js",
    "lint:sass": "sass-lint -v",
    "lint:prettier": "prettier -c .prettierrc \"{pages,store}/**/*.{js,jsx}\" --check",
    "lint": "npm run lint:eslint && npm run lint:sass && npm run lint:prettier",
    "prepare": "husky install",
    "prettier": "prettier -c .prettierrc \"{pages,store}/**/*.{js,jsx}\" --write"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "9.5.3",
    "prop-types": "^15.8.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "react-svg": "^14.1.9",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "sass-lint": "^1.13.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
